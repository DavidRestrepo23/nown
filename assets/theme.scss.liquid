/*================ #Links and Buttons ================*/
a {
	color: $theme-color-default;
	display: inline-block;
	&:hover {
		text-decoration: none;
		color: $theme-color-primary;
		@include transition(all 0.35s ease);
	}
	&.classic-link {
		text-decoration: underline;
	}
}
i {
	display: inline-block;
}

/*================ Buttons ================*/
.btn {
	@include user-select();
	@include prefix(appearance, none, webkit moz spec);
	@include display(inline-block);
	width: auto;
	text-decoration: none;
	text-align: center;
	vertical-align: middle;
	cursor: pointer;
	border: 1px solid transparent;
	@include border-radius(0);
	padding: 10px 20px 10px;
	background-color: $color_button;
	color: $color_button_text;
	font-weight: 600;
	white-space: normal;
	text-transform: uppercase;
	font-size: 11px;
	&:hover {
		background-color: $color_button_hover;
		@include box-shadow(none);
		@include transition(all 0.35s ease);
		border-color: transparent;
	}
	&[disabled], &.disabled {
		cursor: default;
		@inclide opacity(0.6);
		background-color: $color_button;
		color: $color_button_text;
		pointer-events: none;
	}
}
a.btn:hover {
	color: $color-white;
	@include transition(all 0.35s ease);
}

button  {
	&:focus {
		outline: none;
	}
	&[disabled], &.disabled {
		cursor: default;
		@include opacity(0.65);
		background-color: $color_button;
		color: $color_button_text;
		pointer-events: none;
	}
}

.btn:focus {
	outline: 0;
	box-shadow: none;
}

.btn-primary:focus {
	box-shadow: none !important;
}
@media (min-width: 750px) {
	.btn, .shopify-payment-button .shopify-payment-button__button--unbranded {
		padding: 10px 18px;
	}
}
.btn--secondary {
	background-color: transparent;
	color: #557b97;
	border: 1px solid #557b97;
	@include border-radius(2px);
	letter-spacing: 1px;
	font-size: 13px;
	span {
		font-size: 13px;
	}
}

.btn--small {
	font-family: $font-stack-body;
	padding: 8px 10px;
	font-size: em(12);
	line-height: 1;
}
.btn--link {
	background-color: transparent;
	border: 0;
	margin: 0;
	color: $color-body-text;
	@include rtl-text-align-left();
	&:hover,
	&:focus {
		opacity: $opacity-link-hover;
		@include transition(all 0.35s ease);
	}
	.icon {
		vertical-align: middle;
	}
}

.btn--disabled {
	@include opacity(0.5);
	cursor: default;

	&:hover,
	&:focus {
		@include opacity(0.5);
	}
}

.btn-primary:not(:disabled):not(.disabled).active,
.btn-primary:not(:disabled):not(.disabled):active,
.show>.btn-primary.dropdown-toggle {
	color: $color-white;
	background-color: $theme-color-primary;
	border-color: $theme-color-primary;
}

/*================ ACCOUNT ================*/
.account-list {
	padding: 0;
	ul {
		list-style: none;
		padding: 0;
		margin-bottom: 0;
		background: $color-white;
	}
	.close-group-account {
		text-align: center;
		cursor: pointer;
		i {font-size: 24px;color: #222;}
	}
	.account-list-content {
		@include rtl-text-align-left();
		> div, .link_wishlist {
			border-bottom: 1px solid #f4f4f4;
			@include rtl-padding(10px, 15px, 10px, 25px);
			a {
				text-transform: capitalize;
				font-size: 1.2rem;
				&:hover {
					background: $color-white;
					color: $theme-color-primary;
					@include transition(all 0.35s ease);
					i {
						color: $theme-color-primary;
					}
				}
			}
			i {
				color: $theme-color-default;
			    @include rtl-margin-right(20px);
			    min-width: 13px;
			    font-size: 14px;
			}
			span {
				@include display(inline-block);
				vertical-align: middle;
			}
			&.currency-selector {
				border-bottom: none;
				margin-top: 25px;
			}
			&.language-selector {
				border-bottom: none;
			}
		}
		> div:last-child {
			border-bottom: none;
		}
		.groups-selector {
			@include rtl-margin-left(0);
			.dropdown-toggle {
				@include display(none);
			}
			.dropdown-menu {
				@include display(block);
			}
		}
	}
	&.dropdown-menu {
		min-width: 220px;
		@include box-shadow(0 0 10px rgba(0,0,0,0.1));
		border: none;
		overflow: hidden;
		top: 59px !important;
		@include display(block);
		@include opacity(0);
		visibility: hidden;
		@include rtl-right(0);
		@include transition(all 0.3s linear);
		@include rtl-left(inherit !important);
		max-height: 0;
		@include transform(none !important);
		margin: 0;
		@include border-radius(5px);
		&.show {
			@include opacity(1);
			visibility: visible;
			max-height: 285px;
		}
	}
	@media (max-width: 1199px) {
		&.dropdown-menu {
			top: 54px !important;
		}
	}
	@media (max-width: 1199px) {
		&.dropdown-menu {
			top: 78px !important;
		}
	}
}

