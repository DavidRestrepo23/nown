// Slideshow ======================================================================================================== */
.section-slideshow {
    .b_vertical {
    	min-width: 270px;
    	@include rtl-margin-right(10px);
    }
    .b_sls {
    	width: calc(100% - 280px);
    	@include rtl-margin-left(auto);
    }
	.main-slider {
		position: relative;
		width: 100%;
		@include opacity(0);
		visibility: hidden;
		margin-bottom: 0;
		&.slick-initialized {
			@include opacity(1);
			visibility: visible;
		}
	}
	.slick-slide {
		position: relative;
		-webkit-backface-visibility: hidden;
		backface-visibility: hidden;
		>a {
			cursor: default;
		}
		figure {
			position: relative;
			height: 100%;
			margin: 0;
		}
		.slide-image {
			background-size: cover;
			background-position: center;
			@include transition(all .8s ease);
		}
		&.slick-active {
			z-index: 1;
			.content-caption {
				@include opacity(1);
				@include transition(all .7s cubic-bezier(0.32, 0.34, 0, 1.62) .6s);
				margin-top: 0;
			}
		}
	}
	.content {overflow: hidden;}
	.content-caption {
		position: absolute;
		width: 100%;
		z-index: 1;
		@include opacity(0);
		@include transition(all .3s ease);
		padding: 0 20px;
		font-family: $font-family-primary;
		.caption-1 {
			font-weight: 700;
			line-height: 1;
			text-transform: uppercase;
		}
		.caption-2 {
			font-weight: 700;
			line-height: 1;
		}
		.caption-btn {
			font-size: 1.1rem;
			font-weight: 600;
			letter-spacing: 1px;
			padding: 13px 43px 11px;
			text-transform: uppercase;
			&:hover {
				background-color: $theme_color_primary !important;
			}
			@media(max-width: 767px) {
				padding: 9px 20px 7px;
			}
		}
	}
	.slide-image img {
		width: 100%;
	}
	.slick-dots {
		bottom: 25px;
		@include rtl-left(50%);
		@include translateX(-50%);
		width: inherit;
		height: 24px;
		li {
			margin: 0 5px;
			@include square(17px);
			button {
				@include square(17px);
				cursor: pointer;
				@include display(inline-block);
				padding: 0;
				position: relative;
				outline:none;
				&:before{
					content: "";
					position: absolute;
					@include square(17px);
					@include rtl-left(50%);
					top: 50%;
					@include translate(-50%,-50%);
					background-color: #767676;
					@include border-radius(50%);
					@include transition(all 0.2s linear);
				}
			}
			&:hover, &.slick-active {
				button:before {
					@include box-shadow(0 0 0 2px #4b4b4b);
					background-color: white;
				}
			}
		}
		@media(max-width: 767px) {
			bottom: 10px;
		}
	}
	.slick-arrow {
		text-align: center;
		position: absolute;
		top: 50%;
		@include translateY(-50%);
		z-index: 1;
		cursor: pointer;
		background: rgba(#bdbdbd, 0.3);
		padding: 20px 19.8px;
		&:hover {
			background: $theme-color-primary;
			@include transition(all 0.35s ease);
		}
		&.arrow-prev {
			@include rtl-left(0);
		    @include rtl-right(initial);
			@include border-radius(0 9px 9px 0);
		}
		&.arrow-next {
			@include rtl-right(0);
			@include border-radius(9px 0 0 9px);
		}
		i {
			font-size: 2.4rem;
			color: $color-white;
		}
	}
	@media (max-width: 1199px) {
		.b_vertical {
			min-width: 230px;
		}
		.b_sls {
			width: calc(100% - 240px);
		}
	}
	@media (max-width: 991px) {
		.slick-arrow {
			@include opacity(0);
			visibility: hidden;
			@include transition(all 0.3s linear);
		}
		.b_vertical {
			min-width: initial;
			width: 0;
			@include rtl-margin-right(0);
		}
		.b_sls {
			width: 100%;
		}
		&:hover {
			.slick-arrow {
				@include opacity(1);
				visibility: initial;
				@include transition(all 0.3s linear);
			}
		}
	}
	@media (max-width: 500px) {
		.slick-dots, .slick-arrow {
			display: none !important;
		}
		.content-caption .caption-3 a:after {
			margin-top: -1px;
		}
	}
}
// Slideshow split ======================================================================================================== */
.section-slideshow-split {
	.slick-vertical .slick-slide {
		border: none;
		@include display(flex);
	}
	.nov-split-right {
