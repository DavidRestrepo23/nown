	@media(min-width: 992px) {
		position: absolute;
		top: 100%;
		z-index: 99;
		cursor: pointer;
		min-width: 270px;
	}
	@media(max-width: 1199px) {
		min-width: 230px;
		.site-nav {
			>li {
				&>a {
					font-size: 11px;
			        min-height: 40px;
				}
				&.site-nav--has-dropdown {
					.menuGroup {
						.menuCol {
						    margin: 10px 0 0;
						}
						.novMenuLinks {
							padding-top: 15px;
						}
					}
					&.hasMegaMenu .megaMenu {
					    width: 710px !important;
					    @include rtl-margin-left(15px);
					}
					&:hover {
						&>a {
							&:after {
								border-width: 20px 0 20px 15px;
							}
						}
					}
				}
				.site-nav__dropdown {
					@include rtl-margin-left(15px);
				}
			}
		}
		.sub_title_nav {
			font-size: 10px;
		}
		.site-nav--has-dropdown {
			&.hasMegaMenu .megaMenu .novMenuLinks {
				li:not(.menuTitle) a, .site-nav__dropdown {
					.site-nav__link {
						font-size: 11px;
					}
				}
			}
			&.hasMegaMenu .megaMenu {
				.novMenuLinks {
					.menuTitle a {
						font-size: 13px;
						padding: 0;
					}
				}
			}
		}
	}
	@media(max-width: 991px) {
		&:not(#_mobile_vertical_menu) {
			position: fixed;
			height: 100vh;
			overflow: auto;
			top: 0;
			@include rtl-left(-270px);
			z-index: 9999;
			padding: 20px 20px 70px;
			width: 270px;
			@include border-radius(0);
			border: none;
			@include transition(all 0.4s linear);
			background: $color-white;
			.site-nav {
				padding: 0;
			}
			li {
				.site-nav__dropdown {
					position: static;
					width: 100% !important;
					@include opacity(1);
					visibility: visible;
					@include border-radius(0);
					@include box-shadow(none);
					padding: 0;
					background: none !important;
					@include display(none);
					@include transition(none);
				}
			}
			>li>a {
				padding: 0 !important;
			}
			.show_sub {
				display: block !important;
				i {
					font-size: 19px;
				}
			}
			.site-nav>li .megaMenu, .site-nav>li .site-nav__dropdown {
				@include rtl-margin-left(0);
			}
			&.active {
				@include rtl-left(0);
				@include opacity(1);
			}
			.site-nav--has-dropdown ul li {
				@include rtl-padding-left(0);
			}
		}
		.site-nav {
			&>li {
				&>a {
					font-size: 13px;
					padding: 16.6px 0;
					@include display(flex);
					@include align-items(center);
				}
				&:first-child {
					border-top: none;
				}
			}
			&>.site-nav--has-dropdown {
				&>a:before {
					@include display(none);
				}
				&:not(.hasMegaMenu) {
					.site-nav__dropdown {
						ul {
							@include rtl-margin-left(25px);
						}
					}
				}
				&.hasMegaMenu {
					.site-nav__dropdown {
						.menuGroup {
							.novMenuLinks {
								padding-bottom: 10px;
							}
						}
					}
				}
			}
			.site-nav__dropdown {
				li {
					&:last-child {
						a {
							border-bottom: none !important;
						}
					}
					.site-nav__link {
						font-size: 13px;
					}
				}
			}
			.site-nav--has-dropdown.hasMegaMenu .megaMenu .novMenuLinks li:not(.menuTitle) {
				&:last-child a {
					padding: 11px 0;
					border-bottom: none !important;
				}
				a {
					padding: 10px 0;
				}
			}
		}
	}
	@media(max-width: 767px) {
		&#_mobile_vertical_menu {
			li.hide {
				display: block !important;
			}
			.show_more {
				display: none;
			}
		}
		.site-nav>.site-nav--has-dropdown {
			&>a:before {
				@include display(none);
			}
			&.hasMegaMenu .site-nav__dropdown {
				.menuGroup .novMenuLinks {
					padding-top: 0;
				}
			}
		}
	}
}


