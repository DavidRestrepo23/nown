/* Templates | Login =====================================================================================================*/
#CustomerAccountForm, #RecoverPasswordForm, #create_customer, #customer_login {
	label {
		width: 100%;
	}
	.form-group {
		position: relative;
		&:before {
			content: '';
			display: block;
			width: 15px;
			height: 15px;
			position: absolute;
			top: 50%;
			@include translateY(-50%);
			@include rtl-left(15px);
			background: url({{ 'icon-popup-login.png' | asset_url }}) no-repeat;
			background-position: 0 0;
			z-index: 9;
		}
		&:after {
			content: "";
			width: 6px;
			height: 44px;
			background: url({{ 'icon-popup-login.png' | asset_url }}) no-repeat;
			background-position: -19px 0;
			position: absolute;
			top: 1px;
			@include rtl-left(45px);
			z-index: 9;
		}
		&.novform-email {
			&:before {
				width: 16px;
				height: 12px;
				background-position: 0 -18px;
			}
		}
		&.novform-password {
			&:before {
				width: 14px;
				height: 19px;
				background-position: 0 -32px;
			}
		}
		input {
			&:not([type="submit"]) {
				border: 1px solid #e5e5e8;
				padding: 0;
				@include rtl-padding-lr(70px,30px);
				height: 46px;
				width: 100%;
				margin-bottom: 0;
				@include border-radius(2px);
				font-family: $font-family-default;
				font-size: 12px;
				color: $color-heading-sub;
				font-weight: 400;
				@include placeholder{
					font-family: $font-family-default;
					font-size: 11px;
					font-weight: 400;
					color: #cbcbcb;
					text-transform: capitalize;
				}
				&:hover {
					border: 1px solid $theme-color-primary;
				}
			}
		}
	}
	.form-checkbox {
		margin: 21px 0 23px;
		input {
			@include square(16px);
			@include rtl-margin-right(13px);
			margin-bottom: 0;
		}
		label {
			font-family: $font-family-default;
			font-weight: 400;
			font-size: 11px;
			color: #bbbbbb;
			width: initial;
			margin-bottom: 0;
		}
	}
}

#AddressNewForm, .edit_address, .customer_address {
