				}
			}
			&.site-nav--has-dropdown:first-child {
				> .site-nav__dropdown {
					@include display(block);
				}
			}
		}
	}
	&.active {
		@include rtl-left(0);
		.btn_close_sidebar {
			@include opacity(1);
			visibility: visible;
		}
	}
}


