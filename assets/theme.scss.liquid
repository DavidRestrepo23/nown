$transition-link-hover: 0.1s cubic-bezier(0.44, 0.13, 0.48, 0.87);

// Backgrounds
$color-drawer-background: rgba(0, 0, 0, 0.6);
$color-bg-alt: {{ settings.color_body_text | color_modify: 'alpha', 0.05 }};
$color-bg-product: {{ settings.color_bg_product }};

// Border colors
$color-border: {{ settings.color_borders }};

// Product item color
$color-product-item: {{ settings.color_product_item }};
$color-product-name: {{ settings.color_product_name }};
$color-price: {{ settings.color_price }};
$color-reduced-price: {{ settings.color_reduced_price }};
$color-product-add: {{ settings.color_product_addtocart }};
$color-product-icon: {{ settings.color_product_icon }};
$color-badge-new: {{ settings.color_badge_new }};
$color-badge-sale: {{ settings.color_badge_sale }};

// Helper colors for form error states
$color-black: #000;
$color-white: #fff;

/*================ Sizing Variables ================*/
$width-site: 1200px;
$gutter-site: 55px;
$gutter-site-mobile: 22px;
$section-spacing: 30px;
$section-spacing-small: 20px;
$border-radius: 2px;

/*================ Z-Index ================*/
$z-index-dropdown : 7;
$z-index-sub-nav: 8;
$z-index-drawer: 9;
$z-index-skip-to-content: 10000; // really high to be safe of app markup

/*================ SVG ================*/
$svg-select-icon: #{'{{ "ico-select.svg" | asset_url }}'};

/*================ Drawers ================*/
$transition-drawer: all 0.45s cubic-bezier(0.29, 0.63, 0.44, 1);

/*================ Typography ================*/
{% if settings.type_base_family contains 'Google' %}
{% assign type_base_parts = settings.type_base_family | split: '_' %}
{% assign type_base_name = type_base_parts[1] %}
{% capture base_family %}"{{ type_base_name | split: ':' | first | replace: '+', ' ' }}"{% if type_base_parts.last == 'serif' %}, serif {% else %}, "HelveticaNeue", "Helvetica Neue", sans-serif{% endif %}{% endcapture %}
{% else %}
{% assign base_family = settings.type_base_family %}
{% endif %}

{% if settings.type_header_family contains 'Google' %}
{% assign type_header_parts = settings.type_header_family | split: '_' %}
{% assign type_header_name = type_header_parts[1] %}
{% capture header_family %}"{{ type_header_name | split: ':' | first | replace: '+', ' ' }}"{% if type_header_parts.last == 'serif' %}, serif {% else %}, "HelveticaNeue", "Helvetica Neue", sans-serif{% endif %}{% endcapture %}
{% assign header_weight = type_header_parts[2] | split: ',' %}
{% else %}
{% assign header_family = settings.type_header_family %}
{% assign header_weight = 400 %}
{% endif %}

{% if settings.type_slideshow contains 'Google' %}
{% assign type_sideshow_partsconcec = settings.type_slideshow | split: '_' %}
{% assign type_slideshow_name = type_sideshow_partsconcec[1] %}
{% capture slideshow_family %}"{{ type_slideshow_name | split: ':' | first | replace: '+', ' ' }}"{% if type_sideshow_partsconcec.last == 'serif' %}, serif {% else %}, "HelveticaNeue", "Helvetica Neue", sans-serif{% endif %}{% endcapture %}
{% else %}
{% assign slideshow_family = settings.type_slideshow %}
{% endif %}

$font-weight-normal: 400;
$font-weight-bold: 700;

$font-family-default: {{ base_family }};
$font-family-primary: {{ header_family }};

$font-stack-header: $font-family-primary;
$font-weight-header: {{ header_weight }};
$font-size-header: {{ settings.type_header_base_size }};
$font-bold-titles: {{ settings.type_bold_product_titles }};

$font-stack-body: $font-family-default;
$font-size-base: {{ settings.type_base_size }}; // Henseforth known as 1em

$font-stack-cart-notification: "HelveticaNeue","Helvetica Neue",Helvetica,Arial,sans-serif;

$font-size-mobile-input: 16px; // min 16px to prevent zooming


/*================ #Helper Classes ================*/
.clearfix {
	@include clearfix();
}

.visually-hidden {
	@include visually-hidden();
}

.visibility-hidden {
	visibility: hidden;
}

.focus-hidden:focus {
	outline: none;
}
.form-control:focus {
	color: #495057;
	background-color: $color-white;
	border-color: #ced4da;
	outline: 0;
	box-shadow: none;
}

.close {
	@include opacity(1);
}
*{
	outline: none;
}
html {
	font-size:62.5%;
}
@media (max-width: 767px) {
	html {
		font-size:50%;
	}
}
/*============================================================================
	Skip to content button
		- Overrides .visually-hidden when focused
		==============================================================================*/
		.skip-link:focus {
			clip: auto;
			@include square(auto);
			margin: 0;
			color: $color-body-text;
			background-color: $color-white;
			padding: 10px;
			@include opacity(1);
			z-index: $z-index-skip-to-content;
			transition: none;
		}

		/*=============== Lazy loading ===================*/
		.process-loading {
			height: 0;
			@include display(none);
			&:before {
				content: "";
				background: rgba(#fff,0.6);
				top: 70px;
				@include rtl-left(0);
				@include rtl-right(0);
				bottom: 0;
				position: absolute;
				z-index: 1;
			}
			.loader {
				position: absolute;
				@include rtl-left(50%);
				@include translateX(-50%);
				z-index: 2;
				.dot {
					@include square(20px);
					background: $theme-color-primary;
					border-radius: 100%;
					@include display(inline-block);
					animation: nov-slide 1s infinite;
					&:nth-child(1) {
						animation-delay: 0.1s;
						background: rgba($theme-color-primary, 0.2);
					}
					&:nth-child(2) {
						animation-delay: 0.2s;
						background: rgba($theme-color-primary, 0.4);
					}
					&:nth-child(3) {
						animation-delay: 0.3s;
						background: rgba($theme-color-primary, 0.6);
					}
					&:nth-child(4) {
						animation-delay: 0.4s;
						background: rgba($theme-color-primary, 0.8);
					}
					&:nth-child(5) {
						animation-delay: 0.5s;
						background: $theme-color-primary;
					}
				}
			}
			&.active {
				height: 60px;
				@include display(block);
			}
		}

		.box {
			background: no-repeat;
			background-color: #f7f7f7;
			background-size: contain;
		}
		.ratio-container {
			position: relative;
		}
		.ratio-container:after {
			content:'';
			@include display(block);
			height: 0;
			width: 100%;
			padding-bottom: 50%;
			content:"";
		}
		.ratio-container > * {
			position: absolute;
			top: 0;
			@include rtl-left(0);
			@include square(100%);
		}

