				}
				.group_buttons_bottom {
					@include display(flex);
					margin-top: 20px;
				}
				@media(max-width: 767px) {
					.item-product {
						align-items: center;
					}
					.thumbnail-container {
						width: 140px;
					}
					.product__info {
						width: calc(100% - 140px);
						padding-top: 0;
						.product__title {
							margin: 5px 0;
						}
					}
				}
				@media(max-width: 575px) {
					.item-product {
						display: block;
					}
					.thumbnail-container {
						width: 100%;
					}
					.product__info {
						width: 100%;
						margin-top: 20px;
						padding: 0;
					}
				}
			}
		}
		&:not([data-grid="list"]) {
			.group_buttons_bottom .btnProductQuickview, .group_buttons_bottom .btnProductWishlist,.available_product, .desc, .cate {
				display: none !important;
			}
		}
	}
	.nov-pagination {
		margin-top: 60px;
	}
