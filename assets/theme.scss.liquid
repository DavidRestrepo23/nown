		&[data-grid="grid-3"] .product--item {
			@include flex(0 0 33.333333%);
			max-width: 33.333333%;
		}
		&[data-grid="grid-2"] .product--item {
			@include flex(0 0 50%);
			max-width: 50%;
		}
		&[data-grid="list"] {
			.product--item {
				@include flex(0 0 100%);
				max-width: 100%;
				@include flex(0 0 100%);
				max-width: 100%;
				margin-bottom: 30px;
