			.site-nav__dropdown {
				position: static;
				box-shadow: none;
				visibility: visible;
				@include opacity(1);
				border: none;
				@include display(none);
				@include transition(none);
				ul {
					list-style-type: none;
				}
				li {
					border: none;
				}
				.site-nav__link {
					font-size: 11px;
					font-weight: 500;
					padding: 10px 0;
					@include display(block);
					color: $color-text;
					text-transform: uppercase;
					&:hover {
						color: $theme-color-primary;
						background: none;
						@include transition(all 0.35s ease);
