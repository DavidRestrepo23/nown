		}
		button {
			@include square(initial);
			position: relative;
			@include border-radius(0);
			background: transparent;
			padding: 0;
			margin: 0;
			text-shadow: none;
			border: none;
			cursor: pointer;
			@include transition(all 0.35s linear);
			i {
				font-size: 12px;
				font-weight: 400;
				color: $color-white;
			}
			&:hover {
				@include rotate(180deg);
				@include transition(all 0.35s linear);
			}
		}
	}
	.modal-body {
		padding: 10px;
		background: $color-white;
		.wishlistImage {
			@include rtl-margin-right(24px);
		}
		.product-title {
			@include rtl-padding-lr(20px,33px);
			font-family: $font-family-primary;
			font-size: 12px;
			font-weight: 600;
			color: $color-heading;
			a {
				font-family: $font-family-primary;
				font-size: 12px;
				font-weight: 600;
				color: $color-heading;
				&:hover {
					color: $theme-color-primary;
				}
			}
			&:hover {
				color: $theme-color-primary;
			}
		}
		.actions {
			.btn {
				min-width: 100px;
				height: 30px;
				line-height: 30px;
				font-family: $font-family-default;
				font-weight: bold;
				font-size: 10px;
				text-transform: uppercase;
				padding: 0;
				@include border-radius(0);
			}
			.btn-wishlist {
				color: $color-text;
				background: #f1f1f1;
				&:hover {
					background: $theme-color-primary;
					color: $color-white;
				}
			}
		}
	}
	&.novload .modal-body {
		&:after {
			content: "";
			position: absolute;
			@include rtl-left(0);
			bottom: 0;
			width: 0;
			height: 3px;
			background: #c4e8bf;
			animation: novload 5s linear;
		}
	}
	@media(max-width: 400px) {
		&.loading-modal {
			width: 290px;
			bottom: 85px;
			@include rtl-right(15px);
			.modal-body .product-title {
				@include rtl-padding-lr(15px,10px);
			}
		}
	}
}
@keyframes novload {
	0% {
		width: 0;
	}
	100% {
		width: 100%;
	}
}
@-webkit-keyframes novload {
	0% {
		width: 0;
	}
	100% {
		width: 100%;
	}
}
.nov-close {
	font-size: 1.8rem;
	line-height: 10px;
	font-weight: 400;
	color: #b3b3b3;
	@include rotate(0deg);
	position: absolute;
	top: 10px;
	@include rtl-right(10px);
	@include transition(all 0.35s linear);
	text-decoration: none;
	&:hover {
		color: $theme-color-primary;
		outline: none;
		@include rotate(180deg);
		@include transition(all 0.35s linear);
	}
}

// End PopUp ====================================================================================================== */

// Swatch ========================================================================================================= */
.swatch {
	@include align-items(center);
	@include flexbox();
	.swatch-element {
		position: relative;
		display: inline-block;
		@include rtl-margin-right(7px);
		margin-bottom: 4px;
		.crossed-out {
			@include display(none);
		}
		&.soldout {
			.crossed-out {
				@include display(block);
				cursor: no-drop;
			}
		}
		&.color {
			&.white .color_bg {
				border: 1px solid #ebebeb;
			}
			label:not(.color_bg) {
				min-width: 60px;
			}
		}
	}
	input {
		@include opacity(0);
		visibility: hidden;
		height: 0;
	}
	.header {
		font-size: 10px;
		font-family: $font-family-primary;
		font-weight: bold;
		color: $color-heading-second;
		text-transform: uppercase;
		@include rtl-margin-right(15px);
		margin-top: 10px;
		margin-bottom: 5px;
	}
	label {
		min-width: 34px;
		height: 34px;
		line-height: 34px;
		padding: 0 10px;
		text-align: center;
		@include rtl-float-left();
		color: $color-body-text;
		font-size: 12px;
		text-transform: capitalize;
		white-space: nowrap;
		cursor: pointer;
		font-family: $font-family-default;
		@include transition(all 0.2s linear);
		margin-bottom: 0;
		&:not(.color_bg) {
			background-color: transparent !important;
			border: 1px solid #ebebeb;
		}
		&.color_bg {
			.name_color {
				display: none;
			}
		}
		&.color_img {
			height: initial;
			padding: 0;
			.name_color {
				display: none;
			}
		}

	}
	input:checked + label:not(.color_bg), label:not(.color_bg):hover {
		background-color: #ebebeb !important;
	}
	input:checked + label.color_img, label.color_img:hover {
		border-color: #424242;
	}
	input:checked + label.color_bg, label.color_bg:hover {
		border: 1px solid black !important;
		@include box-shadow(0 0 0 1px white inset);
	}
	.crossed-out {
		position: absolute;
		@include square(100%);
		@include rtl-left(0);
		top: 0;
	}
	.tooltip {
		display: none;
	}
	@media (max-width: 375px) {
		.swatch-element:not(.color) label {
			margin-top: 0;
		}
		.header {
			margin-top: 0;
		}
	}
}

// vertical menuTitle
.sidebarmenu_title {
