/*================ #Site Nav and Dropdowns ================*/
.site-nav {
	padding: 0;
	list-style-type: none;
	>li {
		display: inline-block;
		>a {
			font-family: $font-family-primary;
		    color: $color-menu;
		    font-weight: 600;
		    line-height: 1;
		    position: relative;
		    @include transition(all 0.35s ease);
		    display: block;
		    font-size: 1.2rem;
		    text-transform: uppercase;
		    padding: 27px 18px;
		    @media(max-width: 1199px) {
		    	padding-left: 10px;
		    	padding-right: 10px;
		    }
		}
		&:first-child {
			>a {
				@include rtl-padding-left(0);
			}
		}
		&.site-nav--active, &:hover {
			>a {
				color: $theme-color-primary;
			}
		}
		.icon_nav {
			@include rtl-margin-right(5px);
			display: inline-block;
		}
		.group_title {
			display: initial;
		}
	}
}

/*================ Site Nav Links ================*/
.site-nav__link {
	.zmdi-chevron-down {
		@include square(8px);
		@include rtl-margin-left(2px);
	}

	&.site-nav--active-dropdown {
		border: 1px solid $color-border;
		border-bottom: 1px solid transparent;
		z-index: 2;
	}
}
.site-nav--active-dropdown {
	.zmdi-chevron-down {
		&:before {
			content: '\f2fc';
		}
	}
}

.site-nav__dropdown {
    opacity: 0;
    visibility: hidden;
    position: absolute;
    margin: 0;
    z-index: 7;
    background: #fff;
    @include rtl-text-align-left();
    top: calc(100% + 20px);
    @include rtl-left(0);
    @include box-shadow(0 0 15px rgba(0,0,0,0.1));
    @include transition(all 0.35s);
    min-width: 240px;
	.site-nav__link {
		padding: 7px 15px;
		@include transition(all 0.1s);
		font-family: $font-family-default;
		font-size: 1.3rem;
		color: $color-sub-menu;
		display: block;
		&:hover {
			background-color: #f4f4f4;
		}
	}
	.site-nav__childlist-item {
		position: relative;
		.show_sub {
			top: 2px;
		}
		.site-nav__dropdown-children {
			position: absolute;
			@include rtl-left(100%);
			top: 10px;
			opacity: 0;
			visibility: hidden;
			width: max-content;
			min-width: 200px;
			padding: 0 5px 15px;
			@include transition(all 0.3s);
			ul {
				background-color: white;
			}
		}
		&:hover {
			background-color: #f4f4f4;
			.site-nav__dropdown-children {
				@include opacity(1);
				visibility: visible;
				top: -15px;
			}
		}
	}
	.site-nav--active-dropdown {
		@include opacity(1);
		visibility: visible;
		top: 100%;
	}
	@media (max-width: 1199px) {
    	max-width: 760px;
	}
	@media (max-width: 991px) {
    	min-width: 180px;
    	max-width: 690px;
    	.site-nav__childlist-item .site-nav__dropdown-children {
    		min-width: 170px;
    	}
	}
}

/*================ Megamenu ================*/
.site-nav--has-dropdown {
	position: relative;
	@media(min-width: 768px) {
		>a {
			&:after {
				content: "\f2f9";
				font-family: "Material-Design-Iconic-Font";
				color: $color-menu;
				@include rtl-margin-left(7px);
				font-size: 11px;
			}
		}
	}
	.show_sub {
		font-size: 2.4rem;
		color: #b3b4b3;
		cursor: pointer;
		padding: 0 10px;
		line-height: 1;
		position: absolute;
		top: -4px;
		@include rtl-right(0);
		i {
			@include transition(all 0.2s linear);
		}
		&:hover, &.active {
			color: $theme-color-primary;
			i {
				@include rotate(90deg);
			}
		}
	}
	ul {
		padding: 15px 5px 18px;
		li {
			@include display(block);
		}
	}
	>ul>li:last-child > a {
		padding-bottom: 0;
	}
	&:hover {
		>a:after {
			color: $theme_color_primary;
		}
		.site-nav__dropdown {
		    opacity: 1;
		    visibility: visible;
		    top: 100%;
		    z-index: 999;
		}
	}
	&.hasMegaMenu {
		position: static;
		.megaMenu {
			@include rtl-left(50%);
			@include translateX(-50%);
			padding: 10px 40px 15px;
		}
		.menuTitle.has_child {
			margin-bottom: 20px;
			a {
				font-size: 1.4rem;
				font-weight: 700;
				color: $color-title-megamenu;
				text-transform: uppercase;
			}
		}
		.novMenuLinks {
			li {
				margin-bottom: 10px;
			}
			a {
				color: $color-sub-menu;
				&:hover{
					color: $theme_color_primary;
				}
			}
		}
	}
}

/*================ VERTICAL MENU ================*/
@media(min-width: 992px) {
	body:not(.template-index) #_desktop_vertical_menu {display: none;}
}

.title_vertical {
	padding: 30px 30px 20px;
	span {
	    cursor: pointer;
	    font-size: 11px;
	    font-weight: 700;
	    text-transform: uppercase;
	    letter-spacing: 1px;
	}
    i {
    	font-size: 12px;
    	color: $theme-color-primary;
    	vertical-align: middle;
    	@include rtl-margin-right(12px);
    }
}

.vertical_menu {
	.site-nav {
		>li {
			@include display(block);
			position: static;
			list-style-type: none;
			margin: 0;
			max-height: initial;
			padding-left: 30px;
			padding-right: 30px;
			&.hide {
				display: none;
			}
			&:not(:last-child)> a {
				border-bottom: 1px solid #e6e6e6;
			}
			>a {
				font-family: $font-family-primary;
				font-size: 1.3rem;
				font-weight: 500;
				@include transition(all 0.3s linear);
				text-transform: none;
				padding: 18px 0;
				color: $color-body-text;
				display: block;
				&:after {
					display: none;
				}
				&:hover {
					color: $theme-color-primary;
					@include transition(all 0.35s ease);
				}
			}
			&.icon_menu {
				&>a:before {
					display: none;
				}
			}
			&.site-nav--has-dropdown {
				&>a:before {
					display: none;
				}
				.menuGroup {
					.menuCol {
					    margin: 28px 0 0;
