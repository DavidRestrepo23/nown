					.novMenuLinks {
						padding-top: 30px;
						padding-bottom: 30px;
					    @include rtl-padding-left(0);
						margin: 0;
					}
				}
			}
			.megaMenu, .site-nav__dropdown {
				@include rtl-left(100% !important);
				top: 0;
				z-index: 99;
				@include rtl-margin-left(16px);
				@include border-radius(0);
