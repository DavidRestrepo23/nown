    	    &:not(.btn--clear--all) {
    	    	border: 1px solid $color-border;
    	    	padding: 6px 10px 5px;
    	    	font-size: 12px;
    	    	margin-top: 3px;
    	    	margin-bottom: 3px;
    	    	i {
    	    		@include rtl-margin-left(5px);
