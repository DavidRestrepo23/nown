	@media (max-width: 991px) {
		.list-item-chart .tab-content .chart-size table {
			tr.tr_chart td {
				width: 75px;
			}
			&.list-item-2 tr.tr_chart td {
				width: 111px;
			}
		}
	}
	@media (max-width: 767px) {
		.list-item-chart .tab-content .title_tb_chart {
			margin: 30px 0 0;
		}
		.detail_chart {
			display: block;
			.detail_chart_img {
				text-align: center;
				@include rtl-margin-right(0);
				margin-bottom: 50px;
			}
		}
	}
	@media (max-width: 575px) {
		margin-bottom: 45px;
		.block-size_guide {
			margin-bottom: 10px;
		}
	}
	@media (max-width: 425px) {
		.list-item-chart {
			.nav-tabs li.nav-item {
				@include rtl-margin-right(39px);
				&:last-child {
					@include rtl-margin-right(0);
				}
			}
			.tab-content .chart-size table tr.tr_chart {
				td {
					height: 40px;
					&:first-child {
						width: 240px;
						@include rtl-padding-right(10px);
					}
				}
			}
		}
	}
