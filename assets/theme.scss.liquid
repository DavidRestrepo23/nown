	    font-weight: 800;
	    text-transform: uppercase;
	    margin-bottom: 30px;
	    cursor: pointer;
	    padding-top: 3px;
	    &:before, &:after {
	    	content: "";
	    	position: absolute;
	    	top: 50%;
	    	transition: all .5s;
	    	background-color: $color-heading;
	    }
	    &:before {
	    	width: 10px;
    	    height: 2px;
    	    @include rtl-right(0);
    	    transform: translateY(-50%);
	    }
	    &:after {
	    	width: 2px;
    	    height: 10px;
    	    @include rtl-right(4px);
    	    transform: translateY(-50%) rotate(0);
	    }
	    &.act {
	    	&:before {
	    		transform: translateY(-50%) rotate(-180deg);
	    	}
		    &:after {
		    	transform: translateY(-50%) rotate(-90deg);
		    }
		}
