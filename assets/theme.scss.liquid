.categories__sidebar {
	.block__content {
		margin-top: 33px;
		> .cateTitle {
			padding: 9px 0 7px 0;
			border-bottom: 1px solid #e4e9eb;
			&:last-child {
				border: none;
			}
			> a {
				font-family: $font-family-default;
				font-size: 13px;
				color: $color-heading-second;
				font-weight: bold;
				&:hover {
					color: $theme-color-primary;
				}
			}
		}
		.hasSubCategory {
			position: relative;
			&.level1 {
				.arrow {
					position: absolute;
					@include rtl-right(0);
					top: 0;
					@include square(15px);
					margin-top: 9px;
					color: #505050;
					@include border-radius(2px);
					&:after {
						content: "\f273";
						background: #cadae2;
						font-family: 'Material-Design-Iconic-Font';
						@include square(15px);
						min-width: 15px;
						padding: 1px 4px;
						color: $color-white;
						@include border-radius(2px);
					}
					&:hover {
						cursor: pointer;
						&:after {
							background: $theme-color-primary;
						}
					}
					&.collapsed {
						&:after {
							content: "\f278";
						}
					}
				}

			}
			&.level2 {
				.arrow {
					background: none;
					margin-top: 10px;
					&.collapsed {
						background: none;
						top: 50%;
						@include translateY(-50%);
						margin-top: 2px;
						&:hover {
							background: none;
						}
					}
					&:after {
						background: $color-white;
						color: #898989;
					}
					&:hover {
						&:after {
							background: none;
							color: $theme-color-primary;
						}
					}
				}
			}
			&:first-child {
				.arrow {
					top: 0;
				}
			}
		}
		.subCategory {
			margin-top: 8px;
			.cateTitle {
				padding: 3.5px 0;
			}
			.cateItem {
				font-family: $font-family-default;
				color: $color-heading-sub;
				font-size: 13px;
				font-weight: 400;
				&:before {
					content: "";
					display: inline-block;
					@include square(4px);
					background: #6c6c6c;
					@include rotate(45deg);
					@include rtl-margin-right(25px);
					margin-bottom: 2px;
				}
				&:hover {
					color: $theme-color-primary;
				}
			}
		}
		.level2 > .subCategory {
			@include rtl-padding-left(35px);
			.cateItem::before {
				@include display(none);
			}
		}
	}
}
