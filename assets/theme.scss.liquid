/*================ Typography ================*/

body {
	font-size: $font-size-base;
	font-family: $font-family-default;
	color: $color-body-text;
	line-height: 1.5;
	font-weight: 400;
	&.show-boxpage {
		overflow: hidden;
	}
	&.loading {
		position: relative;
		&:before {
			content: '';
			display: block;
			@include square(120px);
			background-image: url({{ 'loading_option.svg' | asset_url }});
			background-repeat: no-repeat;
			background-position: center;
			background-size: cover;
			@include display(inline-block);
			position: fixed;
			top: 50%;
			@include rtl-left(50%);
			transform: translate(-50%,-50%);
			z-index: 999;
		}
		.canvas-overlay {
			@include opacity(1);
			visibility: visible;
		}
    }
    &.cart_popup_opened.loading {
    	.canvas-overlay {
    		@include opacity(0);
			visibility: hidden;
    	}
    }
}

/*================ RTE headings ================*/
.rte {
	color: $color-body-text;
	margin-bottom: $section-spacing-small;
	// If an .rte div is the last element in its parent,
	// make it flush with the bottom of the container
	&:last-child {
		margin-bottom: 0;
	}

	li {
		margin-bottom: 4px;
		list-style: inherit;

		&:last-child {
			margin-bottom: 0;
		}
	}
}

// rte setting type to act like a <p> tag
.rte-setting {
	margin-bottom: 10px; // same as p
	&:last-child {
		margin-bottom: 0;
	}
	p {
		font-size: 1.4rem;
		font-family: $font-family-primary;
		font-weight: 400;
		color: #6c6c6c;
	}
}

/*================ #Icons ================*/
.icon {
	@include display(inline-block);
	@include square(20px);
	vertical-align: middle;
	fill: currentColor;

	.no-svg & {
		@include display(none);
	}
}

svg,
symbol {
	&.icon:not(.icon--full-color) {
		circle,
		ellipse,
		g,
		line,
		path,
		polygon,
		polyline,
		rect {
			fill: inherit;
			stroke: inherit;
		}
	}
}

/*============================================================================
A generic way to visually hide content while
remaining accessible to screen readers (h5bp.com)
==============================================================================*/
.tooltip {
	&.show {
		@include opacity(1);
		padding: 0;
	}
	> .tooltip-inner {
		background-color: $color-white;
		color: $color-heading-sub;
		padding: 5px 20px;
		font-size: 10px;
		text-transform: capitalize;
		margin-bottom: 10px;
		@include border-radius(20px);
		box-shadow: 0 0 5px rgba($color-heading, 0.35);
	}
}

.bs-tooltip-auto[x-placement^=top] .arrow,.bs-tooltip-top .arrow{ 
	bottom: 6px;
}
.bs-tooltip-auto[x-placement^=top] .arrow::before,.bs-tooltip-top .arrow::before{
	bottom: -10px;
	border-width: 5px;
	@include rtl-left(50%);
	@include translateX(-50%);
	border-top-color: $color-white;
}

.shopify-challenge__container {
	max-width: 1200px !important;
	margin: auto;
	background: $color-white;
	padding: 125px 0;
	position: relative;
	z-index: 2;
}
.icon__fallback-text {
	.no-svg & {
