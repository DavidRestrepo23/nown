/* Pagination ==============================================================================================================*/
.nov-pagination {
	margin: 16px 0 80px;
	position: relative;
	&:before {
		content: "";
		position: absolute;
		width: 100%;
		height: 1px;
		background: #e7e7e7;
		top: 50%;
		@include rtl-left(0);
		z-index: 2;
	}
	@media (max-width: 767px) {
		margin: 16px 0 40px;
	}
}
.pagination__viewing {
	font-size: 11px;
	font-family: $font-family-default;
	font-weight: 400;
	color: $color-text;
	text-transform: uppercase;
	background: $color-white;
	z-index: 2;
	@include rtl-padding-right(8px);
	@include rtl-margin-right(auto);
	@media (max-width: 991px) {
		@include rtl-padding-right(15px);
	}
	@media(max-width: 480px) {
		@include display(none);
	}
}

.pagination {
	background: $color-white;
	text-align: center;
	list-style: none;
	z-index: 2;
	@include rtl-padding-left(10px);
	li {
		@include display(inline-block);
		&:not(:last-child) {
			@include rtl-margin-right(5px);
		}
	}
}

.pagination__text {
	>a, >span {
		@include square(40px);
		line-height: 40px;
		@include border-radius(0);
		color: $color-heading;
		font-size: 13px;
		font-weight: bold;
		font-family: $font-family-default;
		display: inline-block;
		border: 1px solid #e4e4e4;
	}
	&.active, &:hover {
		>a, >span {
			background: #e4e4e4;
		}
	}
	&.active {
		&:hover {
			cursor: not-allowed;
		}
	}
}

.pagination__btn {
	font-family: $font-family-default;
	color: #909090;
	font-weight: 500;
	font-size: 11px;
	text-transform: uppercase;
	height: 40px;
	line-height: 40px;
	min-width: 100px;
	padding: 0 5px;
	background: $color-white;
	border: 1px solid #e4e4e4;
	.zmdi-chevron-left {
		@include rtl-margin-right(6px);
	}
	.zmdi-chevron-right {
		@include rtl-margin-left(6px);
	}
	&:hover {
		border: 1px solid $theme-color-primary;
		color: $theme-color-primary;
	}
	&.disabled {
		&:hover {
			cursor: not-allowed;
		}
	}
}

/* Comment =================================================================================================================*/
.article__comments {
	margin: 10px 0 40px;
	.title-block {
		font-family: $font-family-primary;
		font-weight: 600;
		font-size: 2.4rem;
		color: $color-heading;
		margin-bottom: 30px;
	}
	.comments {
		list-style: none;
		margin-top: 20px;
	}
	h5 {
		border-bottom: 1px solid #eaeaea;
		padding-bottom: 6px;
		margin-bottom: 8px;
	}
}
.comment {
	margin-bottom: 20px;
	&:last-child {
		margin-bottom: 0;
	}
}
.comment__content {
	font-family: $font-family-primary;
	font-weight: 400;
	margin-bottom: 5px;
	font-size: 12px;
	line-height: 2rem;
}
.comment__meta-item {
	font-size: 1.4rem;
	&.date {
		font-size: 11px;
		color: $color-text;
		font-weight: 400;
	}
}
.comment-form {
	input, select, textarea {
		border: 1px solid #dfdfdf;
		font-family: $font-family-default;
		font-weight: bold;
		font-size: 12px;
		color: $color-white;
		@include border-radius(2px);
		@include placeholder{
			font-family: $font-family-primary;
			font-weight: 400;
			font-size: 11px;
			color: #bdbdbd;
		}
	}
}


/* Share ===================================================================================================================*/
.btn--share {
	@include display(block);
	.icon {
		vertical-align: middle;
		@include square(16px);
		@include rtl-margin-right(4px);
	}
}

.article__share {
	.dropdown-menu {
		min-width: 125px;
		padding: 10px;
		top: 15px !important;
		@include rtl-left(-30px !important);
		i {
			min-width: 20px;
			@include rtl-margin-right(4px);
			vertical-align: top;
		}
		span {vertical-align: top;}
		a {
			cursor: pointer;
			&:hover {
				color: $theme-color-primary;
			}
			&:not(:last-child) {
				margin-bottom: 10px;
			}
		}
	}
}
.article__share, .article__print {
	padding: 12.5px 30px;
	@include border-radius(2px);
	background: $color-white;
	margin: 1px 0;
	a {
		cursor: pointer;
		font-family: $font-family-primary;
		i {
			@include rtl-margin-right(15px);
			color: $theme-color-primary;
			font-size: 1.3rem;
		}
		span {
			font-size: 11px;
			font-family: $font-family-default;
			color: $color-heading-sub;
			font-weight: bold;
			text-transform: uppercase;
			&:hover {
				color: $theme-color-primary;
			}
		}
	}
	.dropdown-toggle::after {
		@include display(none);
	}
	&:hover {
		background: #eee;
	}
	@media (max-width: 375px) {
		padding: 10px 15px;
	}
}
.icon-facebook {
	fill: #39599f;
}
.icon-twitter {
	fill: #45b0e3;
}
.icon-pinterest {
	fill: #ff4a4a;
}

.share-title {
	@include display(inline-block);
	vertical-align: middle;
	font-family: $font-family-primary;
	font-size: 12px;
	color: #666766;
	font-weight: 700;
	&:hover {
		color: $theme-color-primary;
	}
}

/* Search Bar ==============================================================================================================*/

.overlay{
	position: fixed;
	@include square(100%);
	top: 0;
	@include rtl-left(0);
	z-index: 10000;
	@include opacity(0);
	visibility: hidden;
	@include transition(opacity 0 0.5s,visibility 0 0.5s);
	&.style-dark-bg {
		background-color: rgba($color-heading, 0.6) !important;
	}
	&.style-light-bg {
		background-color: $color-white !important;
	}

	&.overlay-search {
		@include display(table);
		table-layout: fixed;
		@include square(100%);
		text-align: center;
		.search-container form{
			@include opacity(1);
			@include translate(-50%,-50%);
			-webkit-transition: -webkit-transform 0.2s,opacity 0.2s;
			transition: transform 0.2s,opacity 0.2s;
			@include display(inline-block);
			width: 100%;
			max-width: 600px;
			max-width: 60vw;
			position: absolute;
			top: 50%;
			@include rtl-left(50%);
			input {
				@include display(inline-block);
				margin: 0px;
				width: 100%;
				font-size: 28px;
				font-size: 3vh;
				padding: .8em 0;
				@include rtl-padding-lr(1.5em,3em);
				border: none;
				border-bottom: 1px solid #d0d0d0;
				color: $color-heading-sub;
				background-color: transparent;
				border-radius: $border-radius;
				outline: none;
				@include placeholder {
					color: #cfcfcf;
				}
			}
			.search__des {
				color: #343434;
				@include opacity(0);
				@include translateY(-40px);
				@include transition(transform 0.5s,opacity 0.5s);
				-webkit-transition-delay: 0.1s;
				-moz-transition-delay: 0.1s;
				-ms-transition-delay: 0.1s;
				-o-transition-delay: 0.1s;
				transition-delay: 0.1s;
				i {
					font-size: 72px;
				}
				h2 {
					font-size: 36px;
					margin-top: 25px;
					margin-bottom: 20px;
					font-family: $font-family-primary;
					font-weight: bold;
				}
				div {
					font-size: 11px;
					font-weight: 500;
					text-transform: uppercase;
					width: 100%;
					max-width: 440px;
					margin: 0 auto;
					margin-bottom: 85px;
				}
			}
			.search-container-inner {
				position: relative;
				@include opacity(0);
				@include translateY(-40px);
				@include transition(transform 0.3s,opacity 0.3s);
				-webkit-transition-delay: 0.5s;
				-moz-transition-delay: 0.5s;
				-ms-transition-delay: 0.5s;
				-o-transition-delay: 0.5s;
				transition-delay: 0.5s;
			}
		}
		.close-search {
			color: #343434;
			position: absolute;
			top: 50px;
			@include rtl-right(50px);
			font-size: 36px;
			cursor: pointer;
		}
		.search-bar__submit {
			font-size: 28px;
			color: #dfdfdf;
			position: absolute;
			top: 50%;
			@include rtl-right(0.8em);
			@include translateY(-50%);
		}
	}
	&.open {
		@include opacity(1);
		visibility: visible;
		@include transition(opacity 0.5s);
		.search-container form {
			.search__des, .search-container-inner {
				@include opacity(1);
				@include translateY(0);
			}
		}
	}
}

/*============================================================================
The search submit button has pointer-events: none which also
effects the :hover style. This forces the style to be applied.
==============================================================================*/
/* Search Auto Key ==============================================================================================================*/
.search-results {
	background: $color-white;
	@include box-shadow(0 0 10px rgba(#000, 0.15));
	z-index: 9999;
	padding: 15px 0;
	width: 100%;
	min-width: 300px;
	@include border-radius(4px);
	overflow: hidden;
	@include rtl-text-align-left();
	ul {
		list-style-type: none;
		padding: 0;
		&.has-scroll {
			max-height: 500px;
			overflow-y: scroll;
		}
	}
	li {
		padding: 10px 20px;
		border-top: 1px solid #e9e9e9;
		&:first-child {
			border-top: none;
		}
		&:last-child {
			padding: 20px 20px 5px;
		}
		.title {
			font-family: $font-family-primary;
			font-size: 12px;
			color: $color-heading;
			font-weight: 500;
			overflow: hidden;
		    line-height: 20px;
		    display: -webkit-box;
		    -webkit-box-orient: vertical;
		    -webkit-line-clamp: 2;
		    overflow: hidden;
		    margin-bottom: 10px;
			&:hover {
				color: $theme-color-primary;
			}
		}
		.price {
			font-family: $font-family-primary;
			color: $color-heading;
			font-size: 13px;
			font-weight: bold;
		}
	}
	.thumbnail {
		img {
			@include rtl-margin-right(20px);
			@include border-radius(4px);
			border: 1px solid #e9e9e9;
		}
	}
	.see_all {
		font-family: $font-family-default;
		font-size: 10px;
		color: $color-heading;
		text-transform: uppercase;
		font-weight: bold;
	    padding: 10px 0 0;
		&:hover {
			color: $theme-color-primary;
		}
	}
	@media(max-width: 568px) {
		display: none !important;
	}
}

/*================ Header Search ================*/
.site-header__search {
	.search-header__submit {
		position: absolute;
		top: 13px;
		padding: 0;
		border: none;
		z-index: 9;
		color: black;
		cursor: pointer;
		&:hover {
			color: $theme-color-primary;
		}
		i {
			font-size: 17px;
		}
	}
	.site_header__search {
		.search-header__input {
			border: none;
			@include rtl-padding-lr(35px, 15px);
			height: 46px;
			font-size: 12px;
			&:hover, &:focus {
				background: $color-white;
				@include box-shadow(none);
				outline: none;
			}
			@include placeholder {
				font-size: 10px;
				font-weight: 700;
				color: #d5d5d5;
				text-transform: uppercase;
			}
		}
	}
}

// Nov Slick ====================================================================================
.nov-slick-carousel {
	.item img {
		margin: auto;
	}
	.slick-arrow {
		cursor: pointer;
		position: absolute;
		top: 0;
		@include translateY(0);
		z-index: 9;
		color: $theme-color-default;
		i {
			font-size: 4.5rem;
			font-weight: 400;
		}
		&.arrow-next {
			@include rtl-right(0);
		}
		&.arrow-prev {
			@include rtl-right(0);
		}
		&.slick-disabled {
			@include opacity(0.5);
		}
		&:hover {
			color: $theme-color-primary;
		}
	}
	@media (max-width: 767px) {
		.slick-arrow {
			@include display(none !important);
		}
	}
}

// Nov Carousel  ====================================================================================
.nov-owl-carousel {
	.owl-nav {
		[class*='owl-'] {
			background: none;
			padding: 0;
			margin: 0;
			color: $theme-color-default;
			&:hover {
				background: none;
			}
			&.disabled {
				@include display(none);
			}
			&.owl-prev {
				@include rtl-left(-59px);
			}
			&.owl-next {
				@include rtl-right(-59px);
			}
		}
		.owl-prev, .owl-next {
			cursor: pointer;
			position: absolute;
			top: 50%;
			@include translateY(-50%);
			z-index: 9;
			i {
				font-size: 4.5rem;
				font-weight: 400;
				color: $theme-color-default;
			}
			&:hover {
				i {
					color: $theme-color-primary;
				}
			}
		}
	}
	@media(max-width: 767px) {
		.owl-nav {
			@include display(none);
		}
	}
}

/* Product Item ===========================================================================================================*/
.block_margin {
	margin: -30px -30px -60px;
	.block_padding {
		position: static;
		padding: 30px 30px 60px;
		overflow: hidden;
	}
	.slick-list {
		overflow: initial;
		position: relative;
		z-index: 9;
		.slick-slide{
			@include opacity(0);
			visibility: hidden;
			&.slick-active{
				@include opacity(1);
				visibility: visible;
			}
		} 
	}
	.owl-carousel .owl-stage-outer {
		overflow: initial;
	}
	@media (max-width: 575px) {
		margin: -30px -15px -60px;
		.block_padding {
			padding: 30px 15px 60px;
		}
	}
}

.item-product {
	.thumbnail-container {
		position: relative;
		@include transition(all 0.3s linear);
		img {
			margin: 0 auto;
		}
		&.has-multiimage {
			img:not(.product__thumbnail-second) {
				@include opacity(1);
				visibility: initial;
				@include transition(all 0.6s linear);
			}
		}
		a {
			position: relative;
			display: block;
			overflow: hidden;
			text-align: center;
		} 
		.product__thumbnail-second {
			position: absolute;
			top: 0px;
			@include rtl-left(50%);
			@include opacity(0);
			visibility: hidden;
			@include transition(all 500ms);
			@include translate(-50%, 20px);
		}
		.group-buttons {
			position: absolute;
			bottom: 5px;
			@include rtl-right(5px);
		}
		.badge_sale {
			.badge--sale-rt {
				position: absolute;
				top: 10px;
				@include rtl-left(-4px);
				font-family: $font-family-default;
				font-weight: bold;
				font-size: 10px;
				text-transform: uppercase;
				color: $color-white;
				background: $color-badge-new;
				padding: 5.5px 8px;
				max-width: 44px;
				border-radius: 0;
				&:before {
					content: "";
				    width: 0;
				    height: 0;
				    position: absolute;
				    border-style: solid;
				    border-width: 0 0 4px 4px;
				    border-color: transparent transparent darken($color-badge-new, 30%) transparent;
				    top: -4px;
				    @include rtl-left(0);
				}
				.lang-rtl & {
					&:before {
						border-width: 0 4px 4px 0;
				    	border-color: transparent transparent darken($color-badge-new, 30%) transparent;
					}
				}
			}
			.badge--sale-pt {
				position: absolute;
				top: 10px;
				@include rtl-right(-4px);
				font-family: $font-family-default;
				font-weight: bold;
				font-size: 10px;
				text-transform: uppercase;
				color: $color-white;
				background: $color-badge-sale;
				max-width: 44px;
				padding: 5.5px 11px;
				border-radius: 0;
				&:before {
					content: "";
				    width: 0;
				    height: 0;
				    position: absolute;
				    border-style: solid;
				    border-width: 4px 0 0 4px;
				    border-color: transparent transparent transparent darken($color-badge-sale, 30%);
				    top: -4px;
				    @include rtl-right(0);
				}
				.lang-rtl & {
					&:before {
						border-width: 4px 4px 0 0;
				    	border-color: transparent darken($color-badge-sale, 30%) transparent transparent;
					}
				}
			}
		}
	}
	.product__info {
		position: relative;
		.product__title {
			margin: 15px 0 13px;
			font-weight: 500;
			a {
				color: $color-product-name;
				&:hover {
					color: $theme_color_primary !important;
				}
			}
		}
		.product__price {
			color: $color-price;
			font-weight: 700;
			font-family: $font-family-primary;
			s.product-price__price {
				color: $color-reduced-price;
				font-size: 1.3rem;
				@include rtl-margin-left(10px);
			}
		}
		.product__review {
			.spr-badge-caption {
				@include rtl-margin-left(6px);
				font-size: 11px;
				color: #c6c6c6;
			}
			.spr-starrating {
				@include rtl-margin-right(0);
				.spr-icon {
					@include opacity(1);
				}
				.spr-icon:before {
					font-family: "FontAwesome";
					font-size: 12px;
					content: "\f006";
				}
				.spr-icon-star:before {
					color: #ffd201;
				}
				.spr-icon-star-empty:before {
					color: #dbdbdb;
				}
			}
		}
	}
	.product__available {
		text-transform: uppercase;
		font-size: 11px;
		font-weight: 500;
		i {
			@include rtl-margin-right(10px);
			font-size: 1.4rem;
		}
		&.sold-out {
		}
		&.in-stock {
			color: #5fbb52;
		}
	}
	.formAddToCart {
		.btnAddToCart {
			line-height: 40px;
		    height: 40px;
		    @include transition(all 0.3s ease-in-out);
		    text-transform: uppercase;
		    font-size: 1rem;
		    font-weight: 900;
		    text-align: center;
		    background-color: $color-product-add;
		    @include border-radius(0);
		    color: $color-white;
		    padding: 0 5px;
		    width: 100%;
			span {
				font-size: 1.2rem;
				font-weight: 700;
				text-transform: uppercase;
			}
			&:hover {
				background: $theme-color-primary;
				@include box-shadow(0 0 5px 0 rgba(0,0,0,.2));
			}
			.loading {
				display: none;
			}
			&.btn-loading .loading {
				display: inline-block;
        	}
		}
	}
	.productQuickView {
		@include transition(all 0.32s linear);
		&:hover {
			.btnProductQuickview {
				color: $theme_color_primary;
				border: 1px solid $theme-color-primary;
			}
		}
	}
	.productWishList {
		@include transition(all 0.25s linear);
		span {
			display: none;
		}
		&:hover {
			.btnProductWishlist {
				color: $theme_color_primary;
			}
		}
	}
	.productWishList, .productQuickView {
		margin: 0;
		@include translateX(40px);
		@include opacity(0);
		visibility: hidden;
		.btn {
			background: $color-white;
			padding: 0;
			@include square(36px);
			line-height: 36px;
			@include border-radius(3px);
			border: 1px solid #e4e4e4;
			color: $color-product-icon;
			font-size: 14px;
		}
	}
	.productWishList .btnProductWishlist {
		margin-bottom: 5px;
		&.whislist-added, &:hover {
			color: $theme-color-primary;
			border: 1px solid $theme_color_primary;
		}
	}
	&:hover {
		.thumbnail-container {
			z-index: 9;
			&.has-multiimage {
				.product__thumbnail-second {
					@include opacity(1);
					visibility: initial;
					@include translate(-50%, 0);
				}
			}
		}
		.productWishList, .productQuickView {
			@include translateX(0);
			@include opacity(1);
			visibility: initial;
		}
	}
	.grid--view-items & {
		position: relative;
		background: $color-product-item;
		margin-bottom: 25px;
		&:before {
			content: '';
		    position: absolute;
		    width: 100%;
		    height: 100%;
		    top: -15px;
		    @include rtl-left(50%);
		    @include translateX(-50%);
		    background: white;
		    visibility: hidden;
		    @include box-shadow(0 0 2rem rgba(0,0,0, 0.2));
		    @include transition(all 0.2s linear);
		}
		.btnAddToCart {
		    position: absolute;
	        width: 100%;
	        @include rtl-left(0);
	        visibility: hidden;
	        @include opacity(0);
	        @include translateY(15px);
	        @include transition(all 0.2s linear);
	        bottom: -25px;
		    &:focus {
		        outline: none;
		    }
		    &:disabled {
		        cursor: not-allowed;
		        opacity: 0.65 !important;
		    }
		}
		&:hover {
			&:before {
				width: calc(100% + 30px);
			    height: calc(100% + 55px);
			    visibility: visible;
			}
			.btnAddToCart {
				@include translateY(0);
				visibility: visible;
				@include opacity(1);
			}
			.product__price {
				@include opacity(0);
			}
		}
	}
	.list--view-items & {
		margin-bottom: 25px;
		.thumbnail-container {
			max-width: 37.83%;
		}
		.product__info {
			@include rtl-margin-left(30px);
			@include rtl-padding-right(10px);
			.product__title {
				margin-bottom: 20px;
			}
		}
		.formAddToCart .btnAddToCart {
			height: 36px;
			line-height: 36px;
			width: initial;
			min-width: 100px;
			span {
				font-size: 10px;
			}
		}
		.productWishList, .productQuickView {
			@include rtl-margin-left(5px);
			.btn {
				border-radius: 0;
			}
		}
		.group-btn {
			max-height: 0;
			overflow: hidden;
			@include transition(all 0.5s);
		}
		&:hover {
			.group-btn {
				max-height: 150px;
			}
		}
		@media(max-width: 1199px) {
			.product__info .product__title {
				margin-bottom: 5px;
				padding-top: 0;
			}
			.thumbnail-container .badge_sale {
				.badge--sale-rt, .badge--sale-pt {
