                        {% endif %}
                      </div>
                    {% endif %}
                    {% if block.settings.price_4 != blank %}
                      <div class="price-block">{{ block.settings.price_4 }}</div>
                    {% endif %}
                  </div>

                  {% if block.settings.link_4 != blank %}
                    <a class="buy_now" href="{{ block.settings.link_4 }}">{{ 'sections.img-product.buy_now' | t }}</a>
                  {% endif %}
