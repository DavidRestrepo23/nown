                    {% endif %}
                  </div>
                  {% if block.settings.link_2 != blank %}
                    <a class="buy_now" href="{{ block.settings.link_2 }}">{{ 'sections.img-product.buy_now' | t }}</a>
                  {% endif %}
                </div>
              </div>
            </div>
          {% endif %}
          {% if block.settings.image_3 != blank %}
            <div class="{{ block.settings.width_image_3 }} {{ block.settings.width_image_3_tablet }} {{ block.settings.width_image_3_mobile }} {{ block.settings.width_image_3_mobiles }}{% if block.settings.class_3 != blank %} {{ block.settings.class_3 }}{% endif %}">
              <div class="gallery-image__item">
                {% if block.settings.link_3 != blank %}
                  <a class="w-100" href="{{ block.settings.link_3 }}">
                {% endif %}
                  <img class="w-100 img-fluid lazyload" data-src="{{ block.settings.image_3 | img_url: '1920x' }}" alt="image">
                {% if block.settings.link_3 != blank %}
                  </a>
                {% endif %}
                <div class="block_info">
                  <div class="d-flex align-items-center justify-content-between">
                    {% if block.settings.title_3 != blank %}
                      <div class="title-block text-uppercase">
                        {% if block.settings.link_3 != blank %}
                          <a href="{{ block.settings.link_3 }}">
                        {% endif %}
                          {{ block.settings.title_3 }}
                        {% if block.settings.link_3 != blank %}
                          </a>
                        {% endif %}
                      </div>
                    {% endif %}
