                    {% endif %}
                  </div>
                  {% if block.settings.link_3 != blank %}
                    <a class="buy_now" href="{{ block.settings.link_3 }}">{{ 'sections.img-product.buy_now' | t }}</a>
                  {% endif %}
                </div>
              </div>
            </div>
          {% endif %}
