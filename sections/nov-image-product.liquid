<div class="distance{% if section.settings.has_fullwidth == true %} full_width{% endif %}{% if section.settings.class != blank %} {{ section.settings.class }}{% endif %}">
  {% if section.settings.has_fullwidth == false %}
    <div class="container">
  {% endif %}

  {% if section.settings.title != blank %}
  <div class="{{ section.settings.title_alignment }}">
    <div class="title_block {{ section.settings.title_style }}">
      <span>{{ section.settings.title }}</span>
      {% if section.settings.subheading != blank %}
      <span class="sub_title">{{ section.settings.subheading | escape }}</span>
      {% endif %}
    </div>
  </div>
  {% endif %}

  <div class="row spacing-{{ section.settings.spacing }}">
    {% for block in section.blocks %}
      <div class="{{ block.settings.width_group_image }} {{ block.settings.width_group_image_tablet }} {{ block.settings.width_group_image_mobile }} {{ block.settings.width_group_image_mobiles }}{% if block.settings.class_group_image != blank %} {{ block.settings.class_group_image }}{% endif %}">
        <div class="row spacing-{{ section.settings.spacing }}">
          {% if block.settings.image_1 != blank %}
            <div class="{{ block.settings.width_image_1 }} {{ block.settings.width_image_1_tablet }} {{ block.settings.width_image_1_mobile }} {{ block.settings.width_image_1_mobiles }}{% if block.settings.class_1 != blank %} {{ block.settings.class_1 }}{% endif %}">
              <div class="gallery-image__item">
                {% if block.settings.link_1 != blank %}
                  <a class="w-100" href="{{ block.settings.link_1 }}">
                {% endif %}
                <img class="w-100 lazyload" data-src="{{ block.settings.image_1 | img_url: '1920x' }}" alt="img">
                {% if block.settings.link_1 != blank %}
                  </a>
                {% endif %}
                <div class="block_info">
                  <div class="d-flex align-items-center justify-content-between">
                    {% if block.settings.title_1 != blank %}
                      <div class="title-block text-uppercase">
                      {% if block.settings.link_1 != blank %}
                        <a href="{{ block.settings.link_1 }}">
                      {% endif %}
                        {{ block.settings.title_1 }}
                      {% if block.settings.link_1 != blank %}
                        </a>
                      {% endif %}
                      </div>
                    {% endif %}
                    {% if block.settings.price_1 != blank %}
                      <div class="price-block">{{ block.settings.price_1 }}</div>
                    {% endif %}
                  </div>
                  {% if block.settings.link_1 != blank %}
                    <a class="buy_now" href="{{ block.settings.link_1 }}">{{ 'sections.img-product.buy_now' | t }}</a>
                  {% endif %}
                </div>
              </div>
            </div>
          {% endif %}
          {% if block.settings.image_2 != blank %}
            <div class="{{ block.settings.width_image_2 }} {{ block.settings.width_image_2_tablet }} {{ block.settings.width_image_2_mobile }} {{ block.settings.width_image_2_mobiles }}{% if block.settings.class_2 != blank %} {{ block.settings.class_2 }}{% endif %}">
              <div class="gallery-image__item">
                {% if block.settings.link_2 != blank %}
                  <a class="w-100" href="{{ block.settings.link_2 }}">
                {% endif %}
                <img class="w-100 lazyload" data-src="{{ block.settings.image_2 | img_url: '1920x' }}" alt="img">

                {% if block.settings.link_2 != blank %}
                  </a>
                {% endif %}
                <div class="block_info">
                  <div class="d-flex align-items-center justify-content-between">
                    {% if block.settings.title_2 != blank %}
                      <div class="title-block text-uppercase">
                        {% if block.settings.link_2 != blank %}
                        <a href="{{ block.settings.link_2 }}">
                          {% endif %}
                          {{ block.settings.title_2 }}
                        {% if block.settings.link_2 != blank %}
                          </a>
                        {% endif %}
                      </div>
                    {% endif %}
                    {% if block.settings.price_2 != blank %}
